{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "MySQL": "${SCHEDULE_MYSQL_URL}"
  },
  "ApiConfig": {
    "Jwt": {
      "Secret": "${SCHEDULE_JWT_SECRET}"
    },
    "ClientOrigin": "${SCHEDULE_CLIENT_ORIGIN}",
    "Smtp": {
      "Host": "${SCHEDULE_SMTP_HOST}",
      "Port": "${SCHEDULE_SMTP_PORT}",
      "Username": "${SCHEDULE_SMTP_USERNAME}",
      "Password": "${SCHEDULE_SMTP_PASSWORD}",
      "EnableSsl": "${SCHEDULE_SMTP_ENABLE_SSH}",
      "EmailDomain": "${SCHEDULE_SMTP_EMAIL_DOMAIN}"
    },
    "Ssh": {
      "Enabled": "${SCHEDULE_SSH_ENABLED}",
      "Server": "${SCHEDULE_SSH_HOST}",
      "Username": "${SCHEDULE_SSH_LOGIN}",
      "Password": "${SCHEDULE_SSH_PASSWORD}"
    },
    "EmailCapacityMb": "${SCHEDULE_EMAIL_CAPACITY_MB}",
    "MailboxManagerCommand": {
      "Create": "${SCHEDULE_MAILBOX_CREATE_CMD}",
      "SetCapacity": "${SCHEDULE_MAILBOX_SET_CAPACITY_CMD}",
      "Delete": "${SCHEDULE_MAILBOX_DELETE_CMD}"
    },
    "S3": {
      "Url": "${SCHEDULE_S3_HOST}",
      "AccessKey": "${SCHEDULE_S3_ACCESS_KEY}",
      "SecretKey": "${SCHEDULE_S3_SECRET_KEY}"
    },
    "InitAccount": {
      "Name": "${SCHEDULE_INIT_NAME}",
      "Surname": "${SCHEDULE_INIT_SURNAME}",
      "Password": "${SCHEDULE_INIT_PASSWORD}"
    }
  }
}
