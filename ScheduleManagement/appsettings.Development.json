{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "MySQL": "server=localhost;port=${SCHEDULE_DEV_MYSQL_PORT};user=root;password=${SCHEDULE_DEV_MYSQL_PASSWORD};database=${SCHEDULE_DEV_MYSQL_DB_NAME}"
  },
  "ApiConfig": {
    "Jwt": {
      "Secret": "${SCHEDULE_DEV_JWT_SECRET}"
    },
    "ClientOrigin": "http://localhost:${SCHEDULE_DEV_CLIENT_PORT}",
    "Smtp": {
      "Host": "127.0.0.1",
      "Port": "${SCHEDULE_DEV_MAILHOG_API_PORT}",
      "Username": "${SCHEDULE_DEV_MAILHOG_USERNAME}",
      "Password": "${SCHEDULE_DEV_MAILHOG_PASSWORD}",
      "EnableSsl": false,
      "EmailDomain": "schedule.DEV.pl"
    },
    "Ssh": {
      "Enabled": "${SCHEDULE_DEV_SSH_ENABLED}",
      "Server": "${SCHEDULE_DEV_SSH_HOST}",
      "Username": "${SCHEDULE_DEV_SSH_LOGIN}",
      "Password": "${SCHEDULE_DEV_SSH_PASSWORD}"
    },
    "EmailCapacityMb": "${SCHEDULE_DEV_EMAIL_CAPACITY_MB}",
    "MailboxManagerCommand": {
      "Create": "${SCHEDULE_DEV_MAILBOX_CREATE_CMD}",
      "SetCapacity": "${SCHEDULE_DEV_MAILBOX_SET_CAPACITY_CMD}",
      "Delete": "${SCHEDULE_DEV_MAILBOX_DELETE_CMD}"
    },
    "S3": {
      "Url": "http://localhost:${SCHEDULE_DEV_S3_API_PORT}",
      "AccessKey": "${SCHEDULE_DEV_S3_USERNAME}",
      "SecretKey": "${SCHEDULE_DEV_S3_PASSWORD}"
    },
    "InitAccount": {
      "Name": "${SCHEDULE_DEV_INIT_NAME}",
      "Surname": "${SCHEDULE_DEV_INIT_SURNAME}",
      "Password": "${SCHEDULE_DEV_INIT_PASSWORD}"
    }
  }
}
